{
  "name": "linkedout-backend",
  "version": "2.2.2",
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "worker:start": "node dist/worker",
    "worker:start:dev": "nest start --config nest-cli.worker.json --watch",
    "worker:start:debug": "nest start --config nest-cli.worker.json --debug --watch",
    "test": "run-s test:*",
    "test:ts-check": "tsc -p tsconfig.json --noEmit",
    "test:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix --max-warnings=0",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand --detectOpenHandles --forceExit",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "bash -c yarn test:ts-check",
      "bash -c yarn test:eslint"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.142.0",
