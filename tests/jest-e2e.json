{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "testEnvironment": "node",
  "rootDir": ".",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": [
    "node_modules"
  ],
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/../src/$1",
    "tests/(.*)": "<rootDir>/../tests/$1"
  },
  "testTimeout": 60000
}
